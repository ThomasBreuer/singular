 x1:=X(Rationals,"x1");
 x2:=X(Rationals,"x2");
 x3:=X(Rationals,"x3");
 x4:=X(Rationals,"x4");
 x5:=X(Rationals,"x5");
 x6:=X(Rationals,"x6");
 R:=PolynomialRing(Rationals,[x1,x2,x3,x4,x5,x6]);
 T:=X(Rationals,"T");
   1base:=
 [ x1+x2+x3+x4+x5+x6,
x2^2+x2*x3+x2*x4+x2*x5+x2*x6+x3^2+x3*x4+x3*x5+x3*x6+x4^2+x4*x5+x4*x6+x5^2+x5
 *x6+x6^2,
x3^3+x3^2*x4+x3^2*x5+x3^2*x6+x3*x4^2+x3*x4*x5+x3*x4*x6+x3*x5^2+x3*x5*x6+x3*
x6^2+x4^3+x4^2*x5+x4^2*x6+x4*x5^2+x4*x5*x6+x4*x6^2+x5^3+x5^2*x6+x5*x6^2+x6^3-4,
x4^4+x4^3*x5+x4^3*x6+x4^2*x5^2+x4^2*x5*x6+x4^2*x6^2+x4*x5^3+x4*x5^2*x6+x4*x5
 *x6^2+x4*x6^3+x5^4+x5^3*x6+x5^2*x6^2+x5*x6^3+x6^4-4*x4-4*x5-4*x6,
    x5^5+x5^4*x6+x5^3*x6^2+x5^2*x6^3+x5*x6^4+x6^5-4*x5^2-4*x5*x6-4*x6^2,
    x6^6-4*x6^3+7 ];
 elto:=
 x1^2*x3+x1*x2^2+x2*x3^2+x4^2*x6+x4*x5^2+x5*x6^2;
 L:=[];
 LoadPackage("Singular");
# StartSingular();
 SetTermOrdering(R,"lp");
 SingularSetBaseRing(R);
 I:=Ideal(R,1base);
 G:=SingularInterface("std",[I],"ideal");
 L[1]:=SingularInterface("reduce",[elto,G],"poly");
Print(Runtimes(),"\n");
 t1:=Runtime();for i in [2..40] do 
 L[i]:=SingularInterface("reduce",[L[i-1]*L[1],G],"poly"); 
od;t2:=Runtime();
Print("tempo ", StringTime(t2-t1), "\n");
CloseSingular();
Print(Runtimes(),"\n");
